<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    app:cardElevation="4dp"
    app:cardUseCompatPadding="true">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/txt_fragmentViajes_origen"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_weight="2">
            </TextView>
            <TextView
                android:id="@+id/txtFrHistorialViajesOrigen"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:hint="@string/txt_fragmentViajes_origen"
                android:layout_weight="1.9"></TextView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/txt_fragmentViajes_destino"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_weight="2">
            </TextView>
            <TextView
                android:id="@+id/txtFrHistorialViajesDestino"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:hint="@string/txt_fragmentViajes_destino"
                android:layout_weight="1.9"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/txt_fragmentViajes_cupo"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_weight="2">
            </TextView>
            <TextView
                android:id="@+id/txtFrHistorialViajesCupo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:hint="@string/txt_fragmentViajes_cupo"
                android:layout_weight="1.9"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/txt_fragmentViajes_fecha_inicio"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_weight="2">
            </TextView>
            <TextView
                android:id="@+id/txtFrHistorialViajesFechaSalida"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:hint="@string/txt_fragmentViajes_fecha_inicio"
                android:layout_weight="1.9"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/txt_fragmentViajes_hora_salida"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_weight="2">
            </TextView>
            <TextView
                android:id="@+id/txtFrHistorialViajesHoraSalida"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:hint="@string/txt_fragmentViajes_hora_salida"
                android:layout_weight="1.9"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/txt_fragmentViajes_fecha_llegada"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_weight="2">
            </TextView>
            <TextView
                android:id="@+id/txtFrHistorialViajesFechaLlegada"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:hint="@string/txt_fragmentViajes_fecha_llegada"
                android:layout_weight="1.9"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/txt_fragmentViajes_hora_llegada"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_weight="2">
            </TextView>
            <TextView
                android:id="@+id/txtFrHistorialViajesHoraLlegada"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:hint="@string/txt_fragmentViajes_hora_llegada"
                android:layout_weight="1.9"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/txt_fragmentViajes_precio"
                android:textSize="14sp"
                android:textStyle="bold"
                android:layout_weight="2">
            </TextView>
            <TextView
                android:id="@+id/txtFrHistorialViajesPrecio"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:hint="@string/txt_fragmentViajes_precio"
                android:layout_weight="1.9"></TextView>
        </LinearLayout>
    </LinearLayout>

</androidx.cardview.widget.CardView>